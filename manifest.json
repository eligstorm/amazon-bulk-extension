{
  "manifest_version": 3,
  "name": "Amazon Bulk Add to Cart",
  "version": "1.0.0",
  "description": "Add multiple ASINs to Amazon cart with one click.",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}